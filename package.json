{
  "name": "CodeReview",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare:db": "pnpm -C packages/db exec prisma migrate deploy && pnpm -C packages/db exec prisma generate",

    "dev:api": "pnpm --filter @app/api dev",
    "dev:web": "pnpm --filter @app/web dev",
    "dev:worker": "pnpm --filter @app/worker dev",

    "dev:all": "pnpm run prepare:db && concurrently -n API,WEB,WORKER -c cyan,magenta,green \"pnpm dev:api\" \"pnpm dev:web\" \"pnpm dev:worker\"",
    "up:infra": "docker compose up -d",
    "down:infra": "docker compose down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@prisma/client": "^6.15.0",
    "concurrently": "^9.2.1",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "bullmq": "^5.58.2",
    "concurrently": "^8.2.2",
    "dotenv": "^16.6.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  }
}
